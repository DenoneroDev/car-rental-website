<div id="dark-gray">
	<div class="wrapper">
		<footer>
			<img class="logo" id="footer-logo" src="/images/logo-white.png" />
			<div id="footer-content">
				<div id="footer-links">
					[/ if(Array.isArray(links)) {/] [/ links.forEach(link =>
					{ /] [/ if(link.footer) { /]
					<a
						class="footer-link"
						target="_blank"
						href="/page?slug=[/= link.slug /]"
						>[/= link.title /]</a
					>
					[/ } /] [/ }); /] [/} /]
				</div>
				<select id="language">
        			<option value="Deutsch"></option>
        			<option value="English"></option>
        			<option value="Čeština"></option>
      			</select>
			</div>
			<div class="contact" id="footer-contact">
				<a href="mailto:office@vm-car.eu">
					<img src="/images/email-white.png">
					<p>office@vm-car.eu</p>
				</a>
				<div id="both-numbers">
					<img src="/images/phone-white.png">
					<div>
						<a href="tel:[/= numbers.de /]">
							<p>[/= numbers.de /]</p>
						</a>
						<a href="tel:[/= numbers.cs /]">
							<p>[/= numbers.cs /]</p>
						</a>
					</div>
				</div>
			</div>
		</footer>
	</div>
</div>
